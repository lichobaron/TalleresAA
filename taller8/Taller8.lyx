\begin_deeper
\begin_layout Standard

\lang spanish
\begin_inset Formula 
\[
e\left[i,j\right]=\left\{ \begin{array}{rcl}
w(i,j) & ; & i=r\land j=r\\
w(i,j)+e[r+1,j] & ; & i=r\\
{\displaystyle e[i,r-1]+w(i,j)} & ; & j=r\\
e[i,r-1]+e[r+1,j] & ; & i\land j\neq r
\end{array}\right.
\]

\end_inset


\end_layout

\begin_layout Standard

\lang spanish
\begin_inset Formula 
\[
r=max(i,...,j)
\]

\end_inset


\begin_inset Formula 
\[
w\left(i,j\right)=\sum_{l=i}^{j}p_{l}+\sum_{l=i-1}^{j}q_{l}
\]

\end_inset


\end_layout

